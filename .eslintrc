{
    "extends": [
      "airbnb-base"
    ],
    "parserOptions": {
      "ecmaVersion": 6
    },
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "class-methods-use-this": 0,
      "max-len": 0
    }
  }
